# Docker ignore file for Web Audit Suite
# Excludes unnecessary files from Docker build context

# Node modules (will be installed in container)
node_modules/

# Test files and test output
test/
coverage/
*.test.js
test-output/

# Build artifacts and results
results/
.cache/
*.log
error.log
combined.log

# Documentation
docs/
*.md
!README.md

# Git files
.git/
.gitignore

# IDE files
.vscode/
.idea/
*.swp
*~
.DS_Store

# Environment files
.env
.env.local
.env.test

# Examples (keep only in documentation)
examples/

# CI/CD files
.github/
.gitlab-ci.yml

# Temporary files
tmp/
temp/
